;; string-replaceAll function replaces all the "old" occurrences in the "original"
;; string with "new"
(fn string-replaceAll |original old new|
    (string-replace original old new -1)
    )

;; string-count is used to count the number of occurrences of "sub" in the "str"
(fn string-count |str sub|
    ()
    )
